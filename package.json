  "pnpm": {
    "overrides": {
      "vite": "^6.4.1"
    },
    "ignoredBuiltDependencies": [
      "esbuild"
    ]
  },
  "type": "module",
  "dependencies": {
    "@resvg/resvg-js": "^2.6.2",
    "svelte-component-to-image": "^2.0.5",
    "unified": "^11.0.5"
